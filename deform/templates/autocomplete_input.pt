<span tal:omit-tag="">
    <input type="text"
           name="${field.name}"
           value="${cstruct}" 
           tal:attributes="size field.widget.size;
                           class field.widget.css_class"
           id="${field.oid}"/>
    <script tal:condition="field.widget.values" type="text/javascript">
      deform.addCallback(
        '${field.oid}',
        function (oid) {
            $('#' + oid).autocomplete({source: ${values}});
            $('#' + oid).autocomplete("option", ${options});
        }
      );
    </script>
</span>
