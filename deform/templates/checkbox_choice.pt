<div tal:define="css_class css_class|field.widget.css_class;
                 oid oid|field.oid;"
     tal:omit-tag="">
${field.start_sequence()}
<ul class="deformSet">
  <tal:loop tal:repeat="choice values">
    <tal:def tal:define="(value, title) choice">
      <li class="deformSet-item">
        <label tal:define="idx repeat.choice.start and oid or '%s-%s' % (oid, repeat.choice.index)" for="${idx}">
        <input tal:attributes="class css_class;
                               checked value in cstruct"
               type="checkbox"
               name="checkbox"
               value="${value}"
               id="${idx}"/>${title}
        </label>
      </li>
    </tal:def>
  </tal:loop>
</ul>
${field.end_sequence()}
</div>

